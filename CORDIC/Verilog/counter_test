T_2.0 ;
    %vpi_call 2 65 "$display", "Time=%0t: ERROR! Counter is at %d, expected 15.", $time, v0x5af80977d770_0 {0 0 0};
T_2.1 ;
    %load/vec4 v0x5af80977d6d0_0;
    %cmpi/e 0, 0, 1;
